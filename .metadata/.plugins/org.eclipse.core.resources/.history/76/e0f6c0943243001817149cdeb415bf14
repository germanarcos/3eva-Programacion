
public class Admin extends Usuario{

	public Admin(String nombre, String contraEncriptada) {
		super(nombre, contraEncriptada);
	}
	public String getNombreOtro(Usuario user) {
		return user.getNombre();
	}

	public void setNombre(String nombre, Usuario user ) {
		user.setNombre(nombre);
	}

	public String getContraEncriptada(Usuario user) {
		return contraEncriptada;
	}

	public void setContraEncriptada(String contraEncriptada) {
		this.contraEncriptada = contraEncriptada;
	}

}
