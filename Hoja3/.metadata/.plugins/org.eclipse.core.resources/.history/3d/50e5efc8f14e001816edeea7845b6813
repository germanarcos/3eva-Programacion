
public class Hora {
	Integer Hora;
	Integer Minuto;
	public Hora(Integer hora, Integer minuto) {
		Hora = hora;
		Minuto = minuto;
	}
	public Integer getHora() {
		return Hora;
	}
	public void setHora(Integer hora) {
		Hora = hora;
	}
	public Integer getMinuto() {
		return Minuto;
	}
	public void setMinuto(Integer minuto) {
		Minuto = minuto;
	}
	public static Hora horaValida(){
		Hora hora = new Hora(23,00);
		return hora;
	}
}
