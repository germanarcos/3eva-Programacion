import java.util.HashSet;

public class Plato {
	Ingrediente[] ingredientes;
	String nombre;

	Plato(Ingrediente[] ingredientes) {
		this.ingredientes=ingredientes;
	}

	private String nombreAleatorio() {
		int var = ingredientes.size();
		switch (var) {
		case 1:
			return ingredientes.
			break;
		case 2:
			break;
		case 3:
			break;
		case 0:
			return "Pieza de ceramica redonda.";
		}
	}
}
